<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/lib/bootstrap-4.3.1/dist/css/bootstrap.css" type="text/css" />
    <link rel="stylesheet" href="~/css/site.css" type="text/css" />
</head>

<body>
    <header>
        <a asp-controller="home" asp-action="index">Wasteland</a>
        <a asp-controller="home" asp-action="_PartialInspiration">Inspiration</a>
        <a asp-controller="home" asp-action="_PartialHowItWorks">Så funkar det</a>
        <a asp-controller="home" asp-action="_PartialWhyUseWasteland">Varför Wasteland</a>
        <a asp-controller="user" asp-action="Overview">Elin</a>
        <a asp-controller="user" asp-action="Logout">Logga ut</a>
    </header>
    <div>
        @RenderBody()
    </div>
    <footer>
        <p>© All rights reserved: HEH corporation</p>
        <a asp-controller="Info" asp-action="About">Om oss</a>
        <a asp-controller="Info" asp-action="Contact">Kontakt</a>
    </footer>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
</body>
</html>
